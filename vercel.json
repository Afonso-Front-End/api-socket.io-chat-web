{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "BCRYPT": "true"
  },
  "routes": [
    {
      "src": "/socket",
      "dest": "./index.js",
      "headers": {
        "Connection": "Upgrade",
        "Upgrade": "websocket"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
